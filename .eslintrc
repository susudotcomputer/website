{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "globals": {
    "FB": false,
    "React$Element": false,
    "api_oauth_token": false,
    "ga": false,
    "twttr": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "jsx": true,
    "sourceType": "module"
  },
  "plugins": ["jest", "jsx-a11y", "react"],
  "root": true,
  "rules": {
    "guard-for-in": [0],
    "space-infix-ops": [
      2,
      {
        "int32Hint": false
      }
    ],
    "eol-last": [2],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "init-declarations": [0],
    "import/no-duplicates": [2],
    "jest/no-disabled-tests": 1,
    "jest/no-focused-tests": 2,
    "jest/no-identical-title": 2,
    "key-spacing": [0],
    "linebreak-style": [2, "unix"],
    "lines-around-comment": [0],
    "max-nested-callbacks": [0],
    "new-cap": [0],
    "new-parens": [0],
    "newline-after-var": [0],
    "no-alert": [0],
    "no-array-constructor": [0],
    "no-caller": [0],
    "no-case-declarations": [0],
    "no-catch-shadow": [0],
    "no-cond-assign": [1],
    "no-confusing-arrow": [
      1,
      {
        "allowParens": true
      }
    ],
    "no-console": [1],
    "no-debugger": [2],
    "no-duplicate-case": [2],
    "no-duplicate-imports": [0],
    "no-empty": [0],
    "no-empty-character-class": [0],
    "no-empty-label": [0],
    "no-empty-patttern": [0],
    "no-eq-null": [0],
    "no-eval": [0],
    "no-ex-assign": [0],
    "no-extend-native": [0],
    "no-extra-bind": [0],
    "no-extra-boolean-cast": [0],
    "no-extra-parens": [0],
    "no-extra-semi": [0],
    "no-fallthrough": [0],
    "no-floating-decimal": [0],
    "no-func-assign": [0],
    "no-implicit-coercion": [0],
    "no-implied-eval": [0],
    "no-inline-comments": [0],
    "no-inner-declarations": [0],
    "no-invalid-regexp": [0],
    "no-invalid-this": [0],
    "no-irregular-whitespace": [0],
    "no-iterator": [0],
    "no-label-var": [0],
    "no-labels": [0],
    "no-lone-blocks": [0],
    "no-lonely-if": [0],
    "no-loop-func": [0],
    "no-magic-numbers": [0],
    "no-mixed-spaces-and-tabs": [2],
    "no-multi-spaces": [1],
    "no-multi-str": [0],
    "no-multiple-empty-lines": [1],
    "no-native-reassign": [0],
    "no-negated-conditions": [0],
    "no-negated-in-lhs": [0],
    "no-nested-ternary": [0],
    "no-new": [0],
    "no-new-func": [0],
    "no-new-object": [0],
    "no-new-wrappers": [0],
    "no-obj-calls": [1],
    "no-octal": [0],
    "no-octal-escape": [0],
    "no-param-reassign": [0],
    "no-proto": [0],
    "no-redeclare": [1],
    "no-regex-spaces": [0],
    "no-restricted-syntax": [0],
    "no-return-assign": [0],
    "no-script-url": [0],
    "no-self-compare": [0],
    "no-sequences": [0],
    "no-shadow": [0],
    "no-shadow-restricted-names": [0],
    "no-spaced-func": [0],
    "no-sparse-arrays": [0],
    "no-ternay": [0],
    "no-throw-literal": [0],
    "no-trailing-spaces": [2],
    "no-undef": [2],
    "no-undef-init": [2],
    "no-unreachable": [1],
    "no-unused-expressions": [2],
    "no-unused-vars": [2],
    "no-use-before-define": [0],
    "no-useless-call": [0],
    "no-useless-concat": [0],
    "no-var": [0],
    "no-void": [0],
    "no-with": [2],
    "object-curly-spacing": [2, "always"],
    "one-var": [0],
    "operator-assignment": [0],
    "padded-blocks": [0],
    "quote-props": [0],
    "quotes": [2, "single"],
    "react/display-name": [0],
    "react/prop-types": [0],
    "react/jsx-uses-react": [2],
    "react/jsx-uses-vars": [2],
    "require-jsdoc": [0],
    "semi": [2, "always"]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": {
          "resolve": {
            "modules": ["node_modules", "./"]
          }
        }
      }
    }
  }
}
